  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DriverPro</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ScheduledDate">
            <Value>=Parameters!ScheduledDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--Date is till midnight (beginning of day, so one day was added to end date
-- See paramter list in query

SELECT [DCName] 
  ,dc.DCID
  ,CONVERT(decimal(9,3),SUM(ScanQuantity) / SUM(DeliveredQuantity))  AS Scan_Percent
  FROM [DriverPro].[driverpro].[Route] r
  INNER JOIN [DriverPro].[driverpro].[DC] dc ON dc.DCID = r.DCID
  WHERE [ScheduledDate]  = @ScheduledDate AND RouteStatus = 'C'
  GROUP BY [DCName], dc.DCID	
  ORDER BY [DCName]</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="DCName">
          <DataField>DCName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DCID">
          <DataField>DCID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Scan_Percent">
          <DataField>Scan_Percent</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
