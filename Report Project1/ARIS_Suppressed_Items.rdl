  <DataSources>
    <DataSource Name="SygmaODS">
      <DataSourceReference>SygmaODS</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>1671119b-39ea-4306-ae5f-da864a4e8d37</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SygmaODS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@BuyerNumber">
            <Value>=Parameters!BuyerNumber.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT i.ItemUPC, i.ItemName, DCCode, i.ItemCategoryDescription, BuyerName
  FROM erp.Item i 
LEFT JOIN ItemExclude ix on i.ItemNumber = ix.ItemNumber
WHERE ExcludeARIS  = 1 AND ix.BuyerNumber = @BuyerNumber 
    OR 
	(i.ItemCategoryId IN (SELECT CTCODE FROM CategoryExclude )
	  AND ix.IgnoreCatExclude is Null)
ORDER BY i.ItemUPC, DCCode</CommandText>
        <Timeout>12</Timeout>
      </Query>
      <Fields>
        <Field Name="ItemUPC">
          <DataField>ItemUPC</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ItemName">
          <DataField>ItemName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DCCode">
          <DataField>DCCode</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="ItemCategoryDescription">
          <DataField>ItemCategoryDescription</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="BuyerName">
          <DataField>BuyerName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>SygmaODS</DataSourceName>
        <CommandText>SELECT BuyerNumber, BuyerName
  FROM erp.Buyer
   ORDER BY BuyerName</CommandText>
      </Query>
      <Fields>
        <Field Name="BuyerNumber">
