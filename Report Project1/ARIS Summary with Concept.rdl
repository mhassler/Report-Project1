  <DataSets>
    <DataSet Name="ARISHeader">
      <Query>
        <DataSourceName>SygmaODS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DistributionCenters">
            <Value>=Parameters!DistributionCenters.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Buyers">
            <Value>=Parameters!Buyers.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Concepts">
            <Value>=Parameters!Concepts.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@HasOpenPO">
            <Value>=Parameters!HasOpenPO.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT ARISHeader.*
FROM aris.ARISHeader
WHERE DCId IN (@DistributionCenters )
AND BuyerId IN (@Buyers)
AND PrimaryConceptId IN (@Concepts)
AND CASE WHEN @HasOpenPO IS NULL THEN HasOpenPO ELSE @HasOpenPO END = HasOpenPO
ORDER BY PrimaryConceptNumber, InventoryAtRisk DESC</CommandText>
