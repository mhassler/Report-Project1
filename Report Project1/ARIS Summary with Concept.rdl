    <DataSet Name="DistributionCenters">
      <Query>
        <DataSourceName>SygmaODS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Buyers">
            <Value>=Parameters!Buyers.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Concepts">
            <Value>=Parameters!Concepts.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT DCId, DCName
FROM aris.ARISHeader
WHERE BuyerId IN (@Buyers)
AND PrimaryConceptId IN (@Concepts)
ORDER BY DCName</CommandText>
      </Query>
      <Fields>
        <Field Name="DCId">
          <DataField>DCId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DCName">
          <DataField>DCName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Buyers">
      <Query>
        <DataSourceName>SygmaODS</DataSourceName>
        <CommandText>SELECT DISTINCT BuyerId, BuyerNumber, CASE WHEN BuyerName = 'OPEN' THEN BuyerName + '-' + CAST(BuyerNumber AS varchar) ELSE BuyerName END AS BuyerName
FROM aris.ARISHeader
ORDER BY BuyerName</CommandText>
      </Query>
      <Fields>
        <Field Name="BuyerId">
          <DataField>BuyerId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="BuyerNumber">
          <DataField>BuyerNumber</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="BuyerName">
          <DataField>BuyerName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Concepts">
      <Query>
        <DataSourceName>SygmaODS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Buyers">
            <Value>=Parameters!Buyers.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT PrimaryConceptId, PrimaryConceptName
FROM aris.ARISHeader
WHERE BuyerId IN (@Buyers) AND PrimaryConceptId &gt; 0
ORDER BY PrimaryConceptName</CommandText>
      </Query>
      <Fields>
        <Field Name="PrimaryConceptId">
          <DataField>PrimaryConceptId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="PrimaryConceptName">
          <DataField>PrimaryConceptName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
