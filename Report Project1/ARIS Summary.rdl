        </QueryParameters>
        <CommandText>SELECT ARISHeader.*, CASE WHEN HasOpenPO = 1 THEN 'Yes' ELSE 'No' END AS HasOpenPO_Expressive
FROM aris.ARISHeader
WHERE DCId IN (@DistributionCenters )
AND BuyerId IN (@Buyers)
AND CASE WHEN @HasOpenPO IS NULL THEN HasOpenPO ELSE @HasOpenPO END = HasOpenPO
ORDER BY BuyerName, DCName, InventoryAtRisk DESC</CommandText>
