    <DataSet Name="DistributionCenters">
      <Query>
        <DataSourceName>SygmaODS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Buyers">
            <Value>=Parameters!Buyers.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT DCId, DCName
FROM aris.ARISHeader
WHERE BuyerId IN (@Buyers)
ORDER BY DCName</CommandText>
      </Query>
      <Fields>
