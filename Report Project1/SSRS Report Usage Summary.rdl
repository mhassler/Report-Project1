  <DataSources>
    <DataSource Name="SygmaODS">
      <DataSourceReference>SygmaODS</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>df268a95-c095-45f9-b4f9-0ed80be7c666</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SygmaODS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ReportStartDate">
            <Value>=Parameters!ReportStartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ReportEndDate">
            <Value>=Parameters!ReportEndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT ReportName
       ,UsersName
      ,COUNT(*) AS Uses
   FROM [ReportServer].[dbo].[ExecutionLogStorage] l1
   INNER JOIN
   (
   SELECT 
      LogEntryID
      ,name AS ReportName
	 ,CASE WHEN a.[UserName] IS NULL THEN l.UserName ELSE 
	  a.FirstName + ' ' + a.LastName  COLLATE Latin1_General_CI_AS_KS_WS END AS UsersName
  FROM [ReportServer].[dbo].[ExecutionLogStorage] l
  INNER JOIN [ReportServer].[dbo].[Catalog] c ON c.ItemID = l.ReportID  
  LEFT OUTER JOIN [SygmaODS].[dbo].[ADUsers] a 
     ON a.UserName COLLATE Latin1_General_CI_AS_KS_WS  = REPLACE(l.UserName,'NA\',''	) COLLATE Latin1_General_CI_AS_KS_WS 
  WHERE  TimeStart Between @ReportStartDate AND DATEADD(DAY, 1, @ReportEndDate)     
    AND [Status] = 'rsSuccess'
	) x ON x.LogEntryId = l1.LogEntryId 
 GROUP BY ReportName, UsersName
 ORDER BY ReportName, UsersName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ReportName">
          <DataField>ReportName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="UsersName">
          <DataField>UsersName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Uses">
          <DataField>Uses</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
