  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SygmaODS</DataSourceName>
        <CommandText>DECLARE @FiscalYear VARCHAR(4) = '2017'

SELECT * FROM [SygmaODS].[dbo].[getDSOYTD_Concept] ( @FiscalYear )</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ConceptId">
          <DataField>ConceptId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ConceptNumber">
          <DataField>ConceptNumber</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ConceptName">
          <DataField>ConceptName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Column1_Label">
          <DataField>Column1_Label</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DSO_Period1_TotalInventoryValuation">
          <DataField>DSO_Period1_TotalInventoryValuation</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="DSO_Period1_AverageDailySales">
          <DataField>DSO_Period1_AverageDailySales</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="DSO">
          <Value>=Round(Fields!DSO_Period1_TotalInventoryValuation.Value /  Fields!DSO_Period1_AverageDailySales.Value ,2)</Value>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="FiscalPeriods">
      <Query>
        <DataSourceName>SygmaODS</DataSourceName>
        <CommandText>SELECT DISTINCT FiscalYear
FROM erp.FiscalPeriods
ORDER BY FiscalYear DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="FiscalYear">
          <DataField>FiscalYear</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
